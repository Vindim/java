<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="students_pk.old.StudentController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="600.0">
        <items>
          <Button fx:id="addButton" mnemonicParsing="false" text="Добавить" onMouseClicked="#onClickMethod" />
            <Button mnemonicParsing="false" text="Редактировать" />
            <Button mnemonicParsing="false" text="Удалить" />
        </items>
      </ToolBar>
      <TableView fx:id="studentsTable" layoutY="40.0" prefHeight="360.0" prefWidth="600.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Фамилия" fx:id="lastNameColumn" />
          <TableColumn prefWidth="75.0" text="Имя" fx:id="firstNameColumn" />
            <TableColumn prefWidth="75.0" text="Отчество" fx:id="middleNameColumn" />
            <TableColumn prefWidth="75.0" text="Факультет" fx:id="facultyColumn" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
